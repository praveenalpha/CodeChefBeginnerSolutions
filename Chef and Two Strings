#include <iostream>
#include <string>
using namespace std;
int main()
{
    int a;
    cin>>a;
    while(a--)
    {
        int count=0,count1=0;
        string s1,s2;
        cin>>s1>>s2;
        for(int i=0;i<s1.length();i++)
        {
            if(s1[i]=='?' || s2[i]=='?')
            {
                count++;
            }
            else
                if(s1[i]!=s2[i])
                {
                    count1++;
                }
        }
        cout<<count1<<" "<<count1+count<<endl;
        
    }
}
