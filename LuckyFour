#include <bits/stdc++.h>
using namespace std;
int main()
{int i,n,t;

    cin>>t;
    for(i=0;i<t;i++)
    {cin>>n;
    int count=0;
        while(n>0)
        {if(n%10==4)
            {   
                count++;
            }
            n=n/10;
        }
    cout<<count<<endl;
    }
    return 0;
    
}
