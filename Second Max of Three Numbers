#include <bits/stdc++.h>
using namespace std;
void solve(){
    int n;
    cin>>n;
    while(n--){
        int a,b,c;
        cin>>a>>b>>c;
        if(a>b && a<c || a > c && a < b)
            cout<<a<<'\n';
        else if(b>a &&b < c || b > c && b < a)
            cout<<b<<'\n';
        else if(c>a && c<b || c > b && c < a)
            cout<<c<<'\n';
        
    }
}
int main() {
solve();	// your code goes here
	return 0;
}
