#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    string s;
	    cin>>s;
	    int i,c1=0,c2=0;
	    for(i=0;s[i]!='\0';i++){
	        if(s[i]=='1')
	             c1++;
	        else if(s[i]=='0')
	              c2++;
	    }
	    if(c1>c2)
	        cout<<"WIN"<<endl;
	    else
	       cout<<"LOSE"<<endl;
	}
	return 0;
}
