#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        float n,v1,v2,t1,t2;
        cin>>n>>v1>>v2;
        t1=(n*sqrt(2))/v1;
        t2=(2*n)/v2;
        if(t1>t2)
            cout<<"Elevator"<<endl;
        else
            cout<<"Stairs"<<endl;        
        
    }
    return 0;
}
