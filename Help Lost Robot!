#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	int x,y,x1,y1;
	while(t--)
	{
	   cin>>x>>y>>x1>>y1;
	   if((x-x1>0)&&y==y1)
	   cout<<"left"<<endl;
	   else if((x1-x>0)&&y==y1)
	   cout<<"right"<<endl;
	   else if(x==x1&&(y1-y>0))
	   cout<<"up"<<endl;
	   else if(x==x1&&(y-y1>0))
	   cout<<"down"<<endl;
	   else
	   {
	   cout<<"sad"<<endl;
	   }
	   
	}
	return 0;
}
