#include<bits/stdc++.h>
using namespace std;
int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		int b,l,i=1,j=2;
		cin>>b>>l;
		while(1)
		{
			b=b-i;
			i+=2;
			l=l-j;
			j+=2;
			if(b<0)
			{
				cout<<"Bob"<<endl;
				break;
			}
			else if(l<0)
			{
				cout<<"Limak"<<endl;
				break;
			}
		}
	}
	return 0;
}
