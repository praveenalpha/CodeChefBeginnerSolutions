#include <bits/stdc++.h>
using namespace std;
void solve(){
    int n;
    cin>>n;
    while(n--){
        int a,b,c;
        cin>>a>>b>>c;
        if(a>b && a>c)
            cout<<a<<'\n';
        else if(b>a && b > c)
            cout<<b<<'\n';
        else if(c>a && c>b)
            cout<<c<<'\n';
        
    }
}
int main() {
	solve();// your code goes here
	return 0;
}
