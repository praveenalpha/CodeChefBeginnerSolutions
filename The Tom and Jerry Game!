#include <bits/stdc++.h>
using namespace std;
#define lli long long int
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    lli t;
    cin>>t;
    while(t--){
        lli TS;
        cin>>TS;
        while(TS%2==0){
            TS/=2;
        }
        lli JS = TS/2;
        cout<<JS<<'\n';
    }
    return 0;
}
