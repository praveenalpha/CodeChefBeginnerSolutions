#include <bits/stdc++.h>
using namespace std;
#define lli long long int
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    lli t;
    cin>>t;
    while(t--){
        lli TS;
        cin>>TS;
        int x = TS;
        for(lli i=0;i<1000000000000000;i++){
            if(TS%2==0) TS/=2;
            else break;
        }
        lli JS = TS/2;
        cout<<JS<<'\n';
    }
    return 0;
}
