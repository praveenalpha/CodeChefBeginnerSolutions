#include <iostream>
using namespace std;

int main() {
    int l, b;
    cin >> l >> b;
    int area = l*b;
    int peri = 2*(l+b);
    if(area > peri) {
        cout << "Area" << endl;
        cout << area << endl;
    }
    else if(area < peri) {
        cout << "Peri" << endl;
        cout << peri << endl;
    }
    else {
        cout << "Eq" << endl;
        cout << area << endl;
    }
	// your code goes here
	return 0;
}
