#include <iostream>
using namespace std;

int main() {
	int t,d,n,i,j;
	cin>>t;
	
	while(t--)
	{
	    cin>>d>>n; 
	    int sum=0,sum1=0;
	      for(j=1;j<=n;j++)
	      {
	          sum=sum+j;
	      }    
	      n=sum; 
	      if(d>1)
	      {
	      while(d--)
	      { 
	          
	      for(i=1;i<=n;i++)
	      {
	          sum1=sum1+i;
	      }  
	      if(d==1)
	       break;
	       n=sum1; 
	       
	       sum1=0; 
	       
	      } 
	      }
	      else
	      sum1=sum;
	    
	    cout<<sum1<<endl;
	}
	return 0;
}
