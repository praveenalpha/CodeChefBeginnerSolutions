#include <bits/stdc++.h>
#define vi vector<int>
#define pb push_back
#define fast ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
using namespace std;

typedef long long int ll;

ll lcm(ll a, ll b){
    return (a*b)/__gcd(a,b);
}

int main() {
	fast
	int T; cin >> T;
	while(T--){
	    ll a,b;
	    cin >> a >> b;
	    cout << __gcd(a,b) << " " << lcm(a,b) << "\n";
	}
	return 0;
}
