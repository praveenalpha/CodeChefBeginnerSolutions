#include<bits/stdc++.h>
using namespace std;
int main()
{
	string s,c;
	cin>>s;
	int n;
	cin>>n;
	while(n--)
	{
		cin>>c;
		int i,j,p=0;
		for(i=0;i<s.size();i++)
		{
			for(j=0;j<c.size();j++)
			{
				if(c[j]==s[i])
				{
					p++;
				}
			}
 
		}
		if(p==c.size())
		{
			cout<<"Yes"<<endl;
		}
		else
		{
			cout<<"No"<<endl;
		}
	}
	return 0;
}
