#include <bits/stdc++.h>
using namespace std;
int main()
{
	int n,i,arr[100],count_even=0,count_odd=0;
	cin>>n;
	for(i=0;i<n;i++)
	{
		cin>>arr[i];
	}
	for(i=0;i<n;i++)
	{
		if(arr[i]%2==0)
		{
			count_even++;
		}
		else if(arr[i]!=0)
		{
			count_odd++;
		}
	}
	if(count_even>count_odd)
	{
		cout<<"READY FOR BATTLE";
	}
	else
	{
		cout<<"NOT READY";
	}
	return 0;
}
